<h1>Add a restaurant!</h1>
<%= simple_form_for(restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :category, collection: ["chinese", "italian", "japanese", "french", "belgian"] %>
  <%= f.submit %>
<% end %>
<div>
  <%= link_to "Back to restaurants", restaurants_path %>
</div>
